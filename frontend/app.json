{
  "expo": {
    "name": "Bibliapp frontend",
    "slug": "bibliapp-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bibliapp-icon-loading.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B4F72"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png"
      }
    },
    "web": {
      "favicon": "./assets/bibliapp-logo-nav.png"
    },
    "plugins": ["expo-router", "expo-secure-store"]
  }
}
